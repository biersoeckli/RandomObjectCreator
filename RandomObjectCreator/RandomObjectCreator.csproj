<Project Sdk="Microsoft.NET.Sdk">


	<PropertyGroup>
		<TargetFrameworks>net6.0</TargetFrameworks>
		<PackageId>RandomObjectCreator</PackageId>
		<Authors>biersoeckli</Authors>
		<Description>A C# helper util mock objects with random data.</Description>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageTags>RandomObjectCreator, Random, Object, Creator, Test Data, Mock Object</PackageTags>
		<IsPackable>true</IsPackable>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/biersoeckli/RandomObjectCreator</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="Model\" />
		<None Include="..\README.md" Pack="true" PackagePath="\" />
	</ItemGroup>
</Project>
